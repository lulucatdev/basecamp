<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="csrf-token" content={get_csrf_token()} />
    <.live_title default="Basecamp" suffix=" Â· Phoenix Framework">
      {assigns[:page_title]}
    </.live_title>
    <PhoenixVite.Components.assets
      names={["js/app.js", "css/app.css"]}
      manifest={{:basecamp, "priv/static/.vite/manifest.json"}}
      dev_server={PhoenixVite.Components.has_vite_watcher?(BasecampWeb.Endpoint)}
      to_url={&(BasecampWeb.Endpoint.static_url() <> &1)}
    />
  </head>
  <body class="bg-white text-gray-900 antialiased">
    {@inner_content}
  </body>
</html>
